@charset "utf-8";
/* CSS Document */
.load-container {
	width: 60px;
	height: 60px;
	margin:0 auto;
	position: relative;
	overflow: hidden;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

.load8 .loader {
  margin: 0 auto;
  font-size: 10px;
  position:absolute;
  top:20px;
  left:20px;
  text-indent: -9999em;
  border-top: 1.1em solid rgba(146, 184, 49, 0.8);
  border-right: 1.1em solid rgba(146, 184, 49, 0.8);
  border-bottom: 1.1em solid rgba(146, 184, 49, 0.8);
  border-left: 1.1em solid rgba(204, 222, 122, 0.8);
  -webkit-animation: load8 1.1s infinite linear;
  animation: load8 1.1s infinite linear;
}
.load8 .loader,
.load8 .loader:after {
  border-radius: 50%;
  width: 1em;
  height: 1em;
}
@-webkit-keyframes load8 {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}
@keyframes load8 {
  0% {
    -webkit-transform: rotate(0deg);
    transform: rotate(0deg);
  }
  100% {
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}