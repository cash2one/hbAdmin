<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
	<rule>
		<from>^/(\w+)\$</from>
		<to>/$1.html</to>
	</rule>
	<rule>
		<from>^/(\w+)/(\w+)/(\d+)\$</from>
		<to>/$1.html?$2=$3</to>
	</rule>
	<rule>
		<from>^/(\w+)/(\w+)/(\d+)/(\w+)/(\d+)\$</from>
		<to>/$1.html?$2=$3&amp;$4=$5</to>
	</rule>
	<rule>
		<from>^/(\w+)/(\w+)/(\d+)/(\w+)/(\d+)/(\w+)/(\d+)\$</from>
		<to>/$1.html?$2=$3&amp;$4=$5&amp;$6=$7</to>
	</rule>
	<rule>
		<from>^/(\w+)/(\w+)/(\d+)/(\w+)/(\d+)/(\w+)/(\d+)/(\w+)/(\d+)\$</from>
		<to>/$1.html?$2=$3&amp;$4=$5&amp;$6=$7&amp;$8=$9</to>
	</rule>
</urlrewrite>

